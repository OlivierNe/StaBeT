@model string[]
@{
    ViewBag.Title = "Archief";
}

<h2>@ViewBag.Title</h2>

<ul>
    @foreach (var academiejaar in Model)
    {
        <li> @Html.ActionLink("Academiejaar " + academiejaar, "MijnStagesVanAcademiejaar", new { academiejaar = academiejaar })</li>
    }
    @if (Model.Length != 0)
    {
        <li> @Html.ActionLink("Alle academiejaren", "MijnStagesVanAcademiejaar", new { })</li>
    }
</ul>