@{
    ViewBag.Title = "Studenten uit Excel bestand importeren";
}

<h2>@ViewBag.Title</h2>

@using (Html.BeginForm(null, null, FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()
    <p>
        <input type="file" name="file" id="file" accept="application/vnd.openxmlformats-officedocument.spreadsheetml.sheet, application/vnd.ms-excel" />
    </p>
    <p>
        <button type="submit" class="btn btn-default">Importeren</button>
    </p>
}




